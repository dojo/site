{
	"build-app": {
		"build-time-render": {
			"root": "root",
			"paths": [
				"blog",
				"home",
				"community",
				"tutorials/sample-tutorial",
				"tutorials/another-tutorial"
			],
			"puppeteerOptions": {
				"args": [
					"--no-sandbox",
					"--disable-setuid-sandbox"
				]
			}
		},
		"pwa": {
			"manifest": {
				"name": "dojo.io",
				"short_name": "dojo.io",
				"description": "dojo.io",
				"background_color": "#ffffff",
				"theme_color": "#222127",
				"icons": [
					{
						"src": "src/assets/favicon.ico",
						"sizes": [
							128,
							256,
							512
						]
					}
				]
			}
		}
	},
	"create-app": {}
}
