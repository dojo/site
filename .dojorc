{
	"build-app": {
		"locale": "en-us",
		"build-time-render": {
			"root": "root",
			"static": true,
			"renderer": "jsdom",
			"puppeteerOptions": {
				"args": [
					"--no-sandbox",
					"--disable-setuid-sandbox"
				]
			}
		},
		"pwa": {
			"manifest": {
				"name": "dojo.io",
				"short_name": "dojo.io",
				"description": "dojo.io",
				"background_color": "#ffffff",
				"theme_color": "#222127",
				"icons": [
					{
						"src": "src/assets/dojo.png",
						"sizes": [
							16,
							192,
							512
						]
					}
				]
			},
			"serviceWorker": {
				"clientsClaim": true,
				"routes": [
					{
						"urlPattern": ".*",
						"strategy": "networkFirst",
						"expiration": {
							"maxEntries": 25
						}
					}
				]
			}
		}
	},
	"create-app": {}
}
